function pageLoad() {
	advanceAlg();
	document.getElementById("alg-display").visibility = "hidden";
}
function chooseRandomAlg() {
	var algs = ["Aa", "Ab", "E", "F", "Ga", "Gb", "Gc", "Gd", "H", "Ja", "Jb", "Na", "Nb", "Ra", "Rb", "T", "Ua", "Ub", "V", "Y", "Z"];
	var algIndex = Math.floor(Math.random() * algs.length);
	return algs[algIndex];
}
function retrieveAlgImage(oll) {
	var rnd = Math.floor(Math.random() * 57);
	//var second = Math.floor(Math.random() * 4) + 1;
	//return "../images/2SidePLL/svg/" + alg + first + "_" + second + ".svg";
	var ret = algs[rnd];
	ret = ret.replace(/&amp;/g, "&");
	ret = ret.replace(/&#39;/g, "'");
	if(oll) ret = ret + "&stage=oll";
	return ret;
}
function changePll() {
	var algDisplay = document.getElementById("alg-display");
	
	if (algDisplay.style.visibility == "visible") {
		advanceAlg(true);
		algDisplay.style.visibility = "hidden";
	} else {
		document.getElementById("cube-image").src = document.getElementById("cube-image").src.replace("&stage=oll", "");
		algDisplay.style.visibility = "visible";
	}
}
function advanceAlg(oll) {
	var alg = chooseRandomAlg();
	document.getElementById("cube-image").src = retrieveAlgImage(oll);
	//document.getElementById("alg-display").innerHTML = alg;
}